{
  "name": "@skygenesisenterprise/api-sdk",
  "version": "1.0.1",
  "description": "The Official Sky Genesis Enterprise API Service SDK for Node.js Environment",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "build": "echo \"No build step required for this SDK\"",
    "lint": "echo \"No linter configured yet\"",
    "install-deps": "pnpm install",
    "example": "node examples/basic-usage.js",
    "docs": "echo \"Generate documentation\"",
    "prepublishOnly": "npm run test",
    "publish:dry-run": "npm publish --dry-run --access public",
    "prepublish-check": "node scripts/prepublish-check.js",
    "test-npm-config": "node scripts/test-npm-config.js",
    "version:patch": "npm version patch && git push --follow-tags",
    "version:minor": "npm version minor && git push --follow-tags",
    "version:major": "npm version major && git push --follow-tags",
    "release:patch": "npm run version:patch",
    "release:minor": "npm run version:minor",
    "release:major": "npm run version:major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skygenesisenterprise/api-sdkjs.git"
  },
  "keywords": [
    "api",
    "sdk",
    "skygenesis",
    "enterprise",
    "nodejs",
    "stripe-like",
    "backend"
  ],
  "author": "Sky Genesis Enterprise",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "index.js",
    "src/",
    "examples/",
    "README.md",
    "LICENSE"
  ],
  "license": "ISC",
  "private": false,
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "jest": "30.2.0"
  },
  "bugs": {
    "url": "https://github.com/skygenesisenterprise/api-sdkjs/issues"
  },
  "homepage": "https://github.com/skygenesisenterprise/api-sdkjs#readme"
